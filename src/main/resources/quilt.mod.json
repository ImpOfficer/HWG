{
	"schema_version": 1,
	"quilt_loader": {
		"group": "mod.azure.hwg",
		"id": "hwg",
		"version": "${version}",
		"metadata": {
			"name": "Happiness is a Warm Gun",
			"description": "Simple but nice gun mod",
			"contributors": {
				"cybercat5555": "Owner",
				"AzureDoom": "Author"
			},
			"icon": "assets/hwg/icon.png"
		},
		"intermediate_mappings": "net.fabricmc:intermediary",
		"entrypoints": {
			"init": [
				"mod.azure.hwg.HWGMod"
			],
			"client_init": [
				"mod.azure.hwg.client.ClientInit"
			],
			"modmenu": [
				"mod.azure.hwg.config.ModMenuHWG"
			],
			"rei_client": [
				"mod.azure.hwg.rei.ReiPlugin"
			]
		},
		"depends": [
			{
				"id": "quilt_loader",
				"versions": ">=0.17.0-"
			},
			{
				"id": "quilted_fabric_api",
				"versions": ">=2.0.0-"
			},
			{
				"id": "minecraft",
				"versions": ">=1.19.2"
			},
			{
				"id": "geckolib3",
				"versions": ">=3.1.35"
			}
		]
	},
	"mixin": [
		"hwg.mixins.json"
	],
	"minecraft": {
		"environment": "*"
	},
	"access_widener": "hwg.accesswidener"
}
